@inherits LayoutComponentBase

<RadzenDialog />
<RadzenNotification />
<RadzenContextMenu />
<RadzenTooltip />

<CascadingValue Value="@Data" Name="DataSwanShop">
    <RadzenLayout style="grid-template-areas: 'rz-sidebar rz-header' 'rz-sidebar rz-body';">
        <RadzenHeader Style="background-color: rgba(0, 0, 0, 0); color: rgba(0, 0, 0, 0)">
            <div class="container-fluid" style="padding: 0px">
                <div class="row col-12" style="max-width: 100%; max-height: 100%; height: 100%; margin: 0px">
                    <div class="col col-lg-1 col-md-1 col-sm-0 col-xs-0" style="padding: 0px"></div>
                    <div class="col col-lg-4 col-md-4 col-sm-3 col-xs-3" style="padding: 0px"></div>
                    <div class="col col-lg-1 col-md-1 col-sm-2 col-xs-2" style="padding: 0px">
                        <RadzenImage Path="@_currentLogo" Style="float: none; visibility: visible; vertical-align: middle; width: 100%">
                        </RadzenImage>
                    </div>
                    <div class="col col-lg-5 col-md-5 col-sm-3 col-xs-3" style="padding: 0px">
                    </div>
                    <div class="col col-lg-0 col-md-0 col-sm-1 col-xs-1" style="padding: 0px; float: none">
                        <RadzenSidebarToggle Click="@SidebarToggleClick"
                                             Style="text-align: left; float: left; visibility: visible; background-color: rgba(255, 255, 255, 0); border: 0px none #f6f4f4; margin-right: 25px; color: #000000; margin-top: 15px; display: inline-block; vertical-align: middle; margin-left: 25px; margin-bottom: 15px; font-style: normal; text-transform: uppercase">
                        </RadzenSidebarToggle>
                    </div>
                </div>
            </div>
            <div class="row col-12 @collapsed" style="line-height: 33px; vertical-align: middle; padding-bottom: 12px; padding-top: 12px; background-color: rgba(255, 255, 255, 0)">
                <div class="col-lg-1 col-md-1 col-sm-0 col-xs-0" style="padding: 0px; display: block; text-align: center"></div>
                <div class="col-lg-3 col-md-3" style="padding: 0px">
                    <div class="row" style="margin: 0px">
                        <div class="col-2" style="float: right; padding-right: 0px; padding: 0px; text-align: right">
                            <RadzenIcon Style="float: right; display: inline-block; position: relative; vertical-align: middle; width: 30px; height: 30px; font-size: 25px; margin-top: 5px" Icon="search" IconStyle="IconStyle.Primary"></RadzenIcon>
                        </div>
                        <div class="col-10" style="margin: 0px; padding: 0px">
                            <RadzenTextBox Style="border: 1px solid #a4a4a4; border-radius: 15px; margin-top: 1px; margin-left: 1px; padding-bottom: 0px; padding-top: 0px; line-height: 25px; background-color: rgba(0, 0, 0, 0); padding-right: 0px; height: 30px; width: 100%" Placeholder="Search"></RadzenTextBox>
                        </div>
                    </div>
                </div>
                <div class="col-lg-1 col-md-1 col-sm-0 col-xs-0" style="padding: 0px">
                    <RadzenLink Text="Text" Path="/">
                        <RadzenText Text="Home" Style="margin-bottom: 0px; vertical-align: middle; float: none; margin-top: 5px" TextAlign="Radzen.TextAlign.Center" />
                    </RadzenLink>
                </div>
                <div class="col-lg-1 col-md-1 col-sm-0 col-xs-0" style="padding: 0px">
                    <RadzenLink Text="Text" Path="/shop">
                        <RadzenText Text="Shop" TextAlign="Radzen.TextAlign.Center" Style="margin: 0px; margin-top: 5px"></RadzenText>
                    </RadzenLink>
                </div>
                <div class="col-lg-1 col-md-1 col-sm-0 col-xs-0" style="padding: 0px">
                    <RadzenLink Text="Text" Path="/about">
                        <RadzenText Text="About us" TextAlign="Radzen.TextAlign.Center" Style="margin: 0px; margin-top: 5px" />
                    </RadzenLink>
                </div>
                <div class="col-lg-1 col-md-1 col-sm-0 col-xs-0" style="padding: 0px">
                    <RadzenLink Text="Text" Path="/shop">
                        <RadzenText Text="FAQ" TextAlign="Radzen.TextAlign.Center" Style="margin-bottom: 0px; margin-top: 5px" />
                    </RadzenLink>
                </div>
                <div class="col-lg-1 col-md-1 col-sm-0 col-xs-0" style="padding: 0px">
                    <RadzenLink Text="Text" Path="/contact">
                        <RadzenText Text="Contact" TextAlign="Radzen.TextAlign.Center" Style="margin-bottom: 0px; margin-top: 5px" />
                    </RadzenLink>
                </div>
                <div class="col-sm-0 col-xs-0 col-lg-1 col-md-1" style="padding: 0px">
                </div>
                <div class="col-sm-0 col-xs-0 col-lg-2 col-md-2" style="float: none; padding: 0px">
                    <div class="row col-10" style="margin: 0px; float: right; vertical-align: middle">
                        <div class="col-4 justify-content-end" style="display: flex; float: right">
                            <RadzenLink Text="Text" Path="/gestion">
                                <RadzenIcon Style="float: right; font-size: 33px; visibility: visible; padding: 0px" Icon="account_circle" IconStyle="IconStyle.Light"></RadzenIcon>
                            </RadzenLink>
                        </div>
                        <div class="col-6" style="padding: 0px">
                            <RadzenLink Text="Text" Path="/gestion">
                                <RadzenText Text="Login" TextAlign="Radzen.TextAlign.Center" Style="margin-bottom: 0px; float: none; margin-left: 5px; margin-top: 5px; padding: 0px; margin: 0px; text-align: center; display: inline-flex"></RadzenText>
                            </RadzenLink>
                        </div>
                    </div>
                </div>
            </div>
        </RadzenHeader>
        <RadzenBody>
            <div style="padding: 0px">
                <div class="row" style="margin: 0px; width: 100%; max-width: 100%; height: 100%; max-height: 100%">
                    <div class="col" style="border: 0px none #ffffff; background-color: rgba(255, 255, 255, 0); padding: 0px">
                        @Body
                    </div>
                </div>
            </div>
            <RadzenFooter Style="float: none; bottom: 0px; background-color: rgba(0, 0, 0, 0)">
                <div class="row py-2">
                    <div class="col-lg-2 col-md-2 col-sm-0 col-xs-0"></div>
                    <div class="col-lg-2 col-md-2 col-sm-12 col-xs-12">
                        <RadzenLink Path="/shop">
                            <RadzenText Text="Shop" TextAlign="Radzen.TextAlign.Center" Style="margin-top: 2px; margin-bottom: 2px" />
                        </RadzenLink>
                        <RadzenLink Path="/about">
                            <RadzenText Text="About us" TextAlign="Radzen.TextAlign.Center" Style="margin-top: 2px; margin-bottom: 2px" />
                        </RadzenLink>
                        <RadzenLink Path="/contact">
                            <RadzenText Text="Contact" TextAlign="Radzen.TextAlign.Center" Style="margin-top: 2px; margin-bottom: 2px" />
                        </RadzenLink>
                    </div>
                    <div class="col-lg-2 col-md-2 col-sm-0 col-xs-0">
                        <RadzenText Text="Facebook" TextAlign="Radzen.TextAlign.Center" Style="margin-top: 2px; margin-bottom: 2px" />
                        <RadzenText Text="Twitter" TextAlign="Radzen.TextAlign.Center" Style="margin-top: 2px; margin-bottom: 2px" />
                        <RadzenText Text="Instagram" TextAlign="Radzen.TextAlign.Center" Style="margin-top: 2px; margin-bottom: 2px" />
                        <RadzenText Text="Pinterest" TextAlign="Radzen.TextAlign.Center" Style="margin-top: 2px; margin-bottom: 2px" />
                    </div>
                    <div class="col-lg-2 col-md-2 col-sm-0 col-xs-0"></div>
                    <div class="col-lg-2 col-md-2 col-sm-12 col-xs-12">
                        <RadzenText Text="FAQ" TextAlign="Radzen.TextAlign.Center" Style="margin-top: 2px; margin-bottom: 2px" />
                        <RadzenText Text="Shipping & returns" TextAlign="Radzen.TextAlign.Center" Style="margin-top: 2px; margin-bottom: 2px" />
                        <RadzenText Text="Store Policy" TextAlign="Radzen.TextAlign.Center" Style="margin-top: 2px; margin-bottom: 2px" />
                    </div>
                    <div class="col-2"></div>

                </div>
                <div class="row">
                    <RadzenText Text="@_copyRightDate" Style="margin-bottom: 0px; float: left; max-width: 100%; line-height: 20px; font-size: 20px; width: 100%" TextAlign="Radzen.TextAlign.Center" class="pt-4 pb-2" />
                </div>
            </RadzenFooter>
        </RadzenBody>
    </RadzenLayout>
</CascadingValue>