@page "/"

@using BlazorApp.Shared;

@inject HttpClient Http

<PageTitle>@data.PageTitle</PageTitle>


<Banner></Banner>

<MonthCategories></MonthCategories>

<FeaturedProduct></FeaturedProduct>

@code
{
